{
  "name": "todo-server",
  "version": "0.0.4",
  "description": "Webserver for Simple ToDo application",
  "repository": "https://github.com/legaev-stas/simple-todo-server-app",
  "main": "index.js",
  "scripts": {
    "build": "echo [Node.js] Build complete",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dbmigrate": "sequelize db:migrate",
    "dbmigrate-to": "sequelize db:migrate --to",
