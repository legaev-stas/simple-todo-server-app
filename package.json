    "koa-jwt": "^3.3.0",
    "koa-passport": "^4.0.1",
    "koa-router": "^7.4.0",
    "koa-websocket": "^4.1.0",
    "passport-facebook-token": "^3.3.0",
    "pg": "^7.4.1",
    "sequelize": "^4.32.7",
    "sequelize-cli": "^4.0.0"
  }
